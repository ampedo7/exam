{
  "name": "dolores-api",
  "version": "1.0.0",
  "private": true,
  "author": "Keano Carpeso",
  "license": "MIT",
  "homepage": "https://dolores.ph",
  "scripts": {
    "start": "npm run nodemon",
    "nodemon": "nodemon --exec npm run dev",
    "dev": "nodemon dotenv/config  --watch src server.js",
    "production": "nodemon dotenv/config server.js",
    "db:migrate-seed-reset": "yarn db:migrate-reset && yarn db:migrate && yarn db:seedall",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate-rollback": "npx sequelize-cli db:migrate:undo",
    "db:migrate-reset": "npx sequelize-cli db:migrate:undo:all",
    "db:make-migration": "npx sequelize-cli migration:generate",
    "db:make-model": "npx sequelize-cli model:generate",
    "db:make-seeder": "npx sequelize-cli seed:generate",
    "db:seed": "npx sequelize-cli db:seed --seed",
    "db:seed-undo": "npx sequelize-cli db:seed:undo --seed",
    "db:seedall": "npx sequelize-cli db:seed:all",
    "db:seedall-undo": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cli-table": "^0.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-routes-mapper": "^1.1.0",
    "express-validator": "^6.14.0",
    "formidable": "^1.2.2",
    "fs": "^0.0.1-security",
    "hashids": "^2.2.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "object-path": "^0.11.4",
    "qs": "^6.9.4",
    "sequelize": "^5.21.7",
    "slugify": "^1.4.5",
    "uuid": "^8.3.0"
  },
  "_moduleAliases": {
    "@root": ".",
    "@model": "./src/app/models",
    "@controller": "./src/app/controllers",
    "@helpers": "./src/helpers",
    "@services": "./src/services",
    "@routes": "./src/routes"
  },
  "devDependencies": {
    "@sentry/node": "^5.15.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1"
  }
}
